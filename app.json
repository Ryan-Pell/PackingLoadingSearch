{
  "expo": {
    "name": "AB - Packing & Loading Search",
    "slug": "PackingLoadingSearch",
    "version": "0.0.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/SplashScreen.png",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ryanpell.packingloading.searchfind"
    },
    "android": {
      "package": "com.ryanpell.packingloading.searchfind",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#004073"
      },
      "permissions": [
        "CAMERA", "VIBRATE"
      ]
    }
  }
}
